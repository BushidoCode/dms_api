<p id="notice"><%= notice %></p>

<h1>Listing Archdioceses</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @archdioceses.each do |archdiocese| %>
      <tr>
        <td><%= link_to 'Show', archdiocese %></td>
        <td><%= link_to 'Edit', edit_archdiocese_path(archdiocese) %></td>
        <td><%= link_to 'Destroy', archdiocese, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Archdiocese', new_archdiocese_path %>
