<p id="notice"><%= notice %></p>

<h1>Listing Deanery Controllers</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @deanery_controllers.each do |deanery_controller| %>
      <tr>
        <td><%= link_to 'Show', deanery_controller %></td>
        <td><%= link_to 'Edit', edit_deanery_controller_path(deanery_controller) %></td>
        <td><%= link_to 'Destroy', deanery_controller, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

